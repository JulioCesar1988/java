	<queryString>
		<![CDATA[SELECT obras.numero as numero_obra ,
       paquetes.numero as numero_paquete ,
       Paquete_Empleado_aprobado_produccion.fecha,paquetes.descripcion,
       obras.nombre ,
       edificio.nombre as NombreEdificio

FROM (select * from obras where activado = 1 and numero =$P{num_obra} )AS OBRAS INNER JOIN (select * from paquetes where activado = 1 ) AS PAQUETES ON (OBRAS.ID_OBRA = PAQUETES.ID_OBRA)
           INNER JOIN (select * from Paquete_Empleado_aprobado_produccion where activado = 1 ) AS Paquete_Empleado_aprobado_produccion ON (PAQUETES.ID_PAQUETE = Paquete_Empleado_aprobado_produccion.ID_PAQUETE )
           inner join  (select * from edificio where activado = 1 ) as edificio on (  paquetes.id_edificio = edificio.id_edificio)
WHERE OBRAS.NUMERO = $P{num_obra} and paquetes.id_edificio = $P{id_edificio} and  paquetes.id_paquete IN (select id_paquete
						   from Paquete_Empleado_aprobado_produccion
						   where  (activado = 1) and fecha between $P{desde} and $P{hasta}) ]]>
	</queryString>
